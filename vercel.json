{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/v2/guide/",
      "destination": "/guide/introduction.html"
    },
    {
      "source": "/v2/:path",
      "destination": "https://v2.vuejs.org/v2/:path",
      "permanent": false
    }
  ]
}
